function addParagraph(text, elementName){
  var para = document.createElement("p");
  var node = document.createTextNode("Third Paragraoh");
  para.appendChild(node);
  var element = document.getElementById("ninthElement");
  element.appendChild(para);
  }


function writeMessage(message,elementName){
  var myNode = document.getElementById("mynode");
  var childElement = document.createElement(elementName);
  childElement.innerHTML = message;
  myNode.appendChild(childElement);
  }
